{
  "name": "space-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.15.0"
  },
  "scripts": {
    "predev": "yarn i18n",
    "dev": "vite --host",
    "lint": "eslint \"./src/**/*.{js,jsx,vue,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier ./src --write",
    "prebuild": "vue-tsc && yarn lint && unset NODE_OPTIONS && yarn test",
    "build": "vite build",
    "preview": "vite preview",
    "i18n": "google-spreadsheet-vue-i18n --spreadsheet=1akpVJlYvHabXYkz4jG6D3rj7pctWWulwuzGlh5VZVv0 --sheet='Web-superadmin' --format=js --output=src/shared/locales/i18nData.js",
    "prepare": "husky install",
    "test": "vitest --watch false",
    "test:watch": "vitest",
    "precommit": "vue-tsc && lint-staged && yarn test"
  },
  "dependencies": {
    "@vueuse/core": "10.1.2",
    "axios": "1.4.0",
    "chart.js": "4.4.0",
    "file-saver": "2.0.5",
    "js-base64": "3.7.7",
    "maska": "2.1.9",
    "pinia": "2.0.35",
    "print-js": "1.6.0",
    "qs": "6.11.2",
    "universal-cookie": "7.0.1",
    "uuid": "9.0.1",
    "vite-svg-loader": "5.1.0",
    "vue": "3.4.21",
    "vue-chartjs": "5.2.0",
    "vue-i18n": "9",
    "vue-router": "4"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "1.4.1",
    "@types/file-saver": "2.0.7",
    "@types/node": "20.11.5",
    "@types/uuid": "9.0.8",
    "@types/vue-i18n": "7.0.0",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-file-progress": "1.3.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-vue": "9.20.1",
    "google-spreadsheet-vue-i18n": "1.0.3",
    "happy-dom": "13.2.1",
    "husky": "8.0.0",
    "lint-staged": "15.2.2",
    "naive-ui": "2.38.1",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "typescript": "5.0.2",
    "unplugin-auto-import": "0.17.3",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.0.12",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "1.2.1",
    "vue-tsc": "1.4.2"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  }
